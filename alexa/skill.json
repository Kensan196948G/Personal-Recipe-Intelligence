{
  "manifest": {
    "publishingInformation": {
      "locales": {
        "ja-JP": {
          "name": "パーソナル レシピ インテリジェンス",
          "summary": "料理レシピを音声で管理・ナビゲーション",
          "description": "Personal Recipe Intelligence（PRI）は、料理レシピの音声管理とハンズフリーナビゲーションを提供するスキルです。材料の読み上げ、手順のナビゲーション、タイマー設定などが可能です。",
          "examplePhrases": [
            "アレクサ、パーソナルレシピを開いて",
            "次の手順を教えて",
            "材料を読んで"
          ],
          "keywords": [
            "料理",
            "レシピ",
            "クッキング",
            "ナビゲーション",
            "タイマー"
          ]
        }
      },
      "isAvailableWorldwide": false,
      "testingInstructions": "「アレクサ、パーソナルレシピを開いて」と話しかけてください。その後、レシピのナビゲーションコマンドを試してください。",
      "category": "FOOD_AND_DRINK",
      "distributionCountries": ["JP"]
    },
    "apis": {
      "custom": {
        "endpoint": {
          "uri": "arn:aws:lambda:ap-northeast-1:ACCOUNT_ID:function:PersonalRecipeIntelligence"
        },
        "interfaces": []
      }
    },
    "manifestVersion": "1.0",
    "privacyAndCompliance": {
      "allowsPurchases": false,
      "usesPersonalInfo": false,
      "isChildDirected": false,
      "isExportCompliant": true,
      "containsAds": false,
      "locales": {
        "ja-JP": {
          "privacyPolicyUrl": "https://example.com/privacy",
          "termsOfUseUrl": "https://example.com/terms"
        }
      }
    }
  },
